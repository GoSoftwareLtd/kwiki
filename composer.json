{
    "name": "fungku/kwiki",
    "description": "Quick wiki or blog with markdown files",
    "keywords": ["wiki", "blog", "markdown"],
    "license": "Apache-2.0",
    "type": "project",
    "require": {
        "laravel/lumen-framework": "5.0.*",
        "vlucas/phpdotenv": "~1.0",
        "fungku/postmark": "^0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Fungku\\Kwiki\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
